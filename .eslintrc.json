{
    "parser": "babel-eslint",
    "extends": ["airbnb-base", "plugin:react/recommended", "plugin:prettier/recommended"],
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest/globals": true
    },
    "plugins": [
        "react",
        "jest",
        "jsx-a11y",
        "import"
    ],
    "globals": {
        "graphql": true
    },
  "ignorePatterns": ["public/", "flow-typed/", "node_modules/"]
}